<ul class="collapsible">
  <% for (var key in folders) { %>
    <li>
      <div class="collapsible-header black"><i class="material-icons">library_music</i><%= key %></div>
      <div class="collapsible-body">
        <div class="row">
          <% for (var k in folders[key]) { %>
            <div class="col s12 m6"> 
              <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                  <span class="card-title"><%= folders[key][k] %></span>
                  <p>
                    <audio controls controlsList="nodownload">
                      <source src="<%= directoryPath %>/<%= key %>/<%= folders[key][k] %>" type="audio/mpeg">
                    </audio>
                  </p>
                </div>
              </div> 
            </div>  
          <% } %>
        </div>
      </div>
    </li>
  <% } %>
</ul>