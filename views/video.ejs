<%- include includes/header %>
    <h1>Lista de pel√≠culas</h1>
    <div class="row">
      <% Object.keys(videos).forEach(function(file, index){ %>
        <div class="col-sm">
          <div class="card movie">
            <img class="card-image-top" src="<%= directoryPath + '/' + file + '/' + videos[file]%>" alt="<%= file %>">
            <div class="card-body">
              <h4 class="card-title"><%= file %></h4>
              <a class="btn btn-primary" href="/video/<%= file %>">Ver</a>
            </div>  
          </div>
        </div>
      <% }); %>
    </div>
<%- include includes/footer %>